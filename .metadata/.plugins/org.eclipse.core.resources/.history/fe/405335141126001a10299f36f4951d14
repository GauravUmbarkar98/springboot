package util;

public class Test {
	public static void main(String[] args) {
		
		
		Animal a = new Cow();
		a.walk();
	}
}

abstract class Animal
{
	abstract void walk();
	int add(int i, int j) {
		return i+j;
	}
	
}
class Cow extends Animal
{
	public void walk() {
		System.out.println("walking....cow");
	}
}

