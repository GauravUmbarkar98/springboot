package util;

public class Test {
	public static void main(String[] args) {
		
		
		Animal a = new Cow();
		a.walk();
	}
}

interface Animal
{
	void walk(); 
	
}
class Cow implements Animal
{
	public void walk() {
		System.out.println("walking....");
	}
}

